<?xml version="1.0" encoding="utf-8"?>
<!-- 
    注意: 除了手動編輯這個檔案以外，您也可以使用 
    Web 管理工具設定您的應用程式設定值。請使用 
    Visual Studio 中的 [網站] -> [ASP.NET 組態] 選項。
    如需完整的設定與註解清單，請參考 
    machine.config.comments (通常位於 
    \Windows\Microsoft.Net\Framework\v2.x\Config) 
-->
<configuration>
	<appSettings/>
	<connectionStrings/>
	<system.web>
		<pages validateRequest="false" enableEventValidation="false" />
	</system.web>

	<!--Role Management-->
	<location path="Design/AuthoritySetting.aspx">
		<system.web>
			<authorization>
				<allow roles="QueAdmin,SystemAdmin"/>
				<deny users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="Design/QusProperty.aspx">
		<system.web>
			<authorization>
				<allow roles="QueManager,QueAdmin,SystemAdmin,QueClassDesigner"/>
				<deny users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="Design/QusPropertyMA.aspx">
		<system.web>
			<authorization>
				<allow roles="QueManager,QueAdmin,SystemAdmin,QueClassDesigner"/>
				<deny users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="Design/QusDesign.aspx">
		<system.web>
			<authorization>
				<allow roles="QueManager,QueAdmin,SystemAdmin,QueClassManager,QueClassDesigner"/>
				<deny users="*"/>
			</authorization>
		</system.web>
	</location>
  <location path="Design/QusExamine.aspx">
		<system.web>
			<authorization>
				<allow roles="QueExaminer, SystemAdmin,QueClassExaminer"/>
				<deny users="*"/>
			</authorization>
		</system.web>
	</location>

</configuration>

