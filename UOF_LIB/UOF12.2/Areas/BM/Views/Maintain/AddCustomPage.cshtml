<script>


    $(function () {
        $('form').submit(function () {
            var a = $('#TextType').val(); //抓取欄位樣式SelectValue

            var l_CUSTOM_FIELD_TYPE = $('#CUSTOM_FIELD_TYPE').val();
            var l_FieldType = $('#' + a).find("input[name^='FieldType']").val(); //,
            var l_FieldName = $('#' + a).find("input[name^='FieldName']").val(); //,
            var l_IsRequiredField = $('#' + a).find("input[name^='IsRequiredField'] "); //,
            var l_IsReadOnly = $('#' + a).find("input[name^='IsReadOnly'] "); //,
            var l_FieldRemark = $('#' + a).find("input[name^='FieldRemark']").val(); //,

            var l_FieldValue = '';

            if (a == 'MultiLineText') {
                l_FieldValue = $('#' + a).find("textarea[name^='FieldValue']").val(); //,

            } else {
                l_FieldValue = $('#' + a).find("input[name^='FieldValue']").val(); //,

            }


            var l_FieldLength = $('#' + a).find("input[name^='FieldLength']").val(); //,
            var l_FieldRowCount = $('#' + a).find("input[name^='FieldRowCount']").val(); //,
            var l_MaxValue = $('#' + a).find("input[name^='MaxValue']").val(); //,v
            var l_MinValue = $('#' + a).find("input[name^='MinValue']").val(); //,
            var l_RepeatColumns = $('#' + a).find("input[name^='RepeatColumns']").val(); //,
            var l_ListItem = $('#' + a).find("input[name^='ListItem']");
            var l_Field_ID = $('#CTB_EIP_BM_CUSTOM_FIELD_XML_SCHEMA_0__FieldId').val(); //

      


            var array = [];

            var JSON_ARRAY = {
                CustomFiledType: [],
                FieldType: [],
                FieldName: [],
                FieldRemark: [],
                FieldValue: [],
                FieldLength: [],
                FieldRowCount: [],
                MaxValue: [],
                MinValue: [],
                RepeatColumns: [],
                IsRequiredField: [],
                IsReadOnly: [],
                ListItem: [],
                FieldId: []
            }

            JSON_ARRAY.CustomFiledType = l_CUSTOM_FIELD_TYPE;
            JSON_ARRAY.FieldLength = l_FieldLength;
            JSON_ARRAY.FieldName = l_FieldName;
            JSON_ARRAY.FieldRemark = l_FieldRemark;
            JSON_ARRAY.FieldRowCount = l_FieldRowCount;
            JSON_ARRAY.FieldType = l_FieldType;
            JSON_ARRAY.FieldValue = l_FieldValue;
            JSON_ARRAY.MaxValue = l_MaxValue;
            JSON_ARRAY.MinValue = l_MinValue;
            JSON_ARRAY.RepeatColumns = l_RepeatColumns;
            JSON_ARRAY.IsRequiredField = l_IsRequiredField.is(':checked');
            JSON_ARRAY.IsReadOnly = l_IsReadOnly.is(':checked');
            JSON_ARRAY.FieldId = l_Field_ID;

            if (l_ListItem.length > 0) {
                for (var i = 0 ; i < l_ListItem.length; i++) {
                    array.push(l_ListItem[i].value);
                }
                JSON_ARRAY.ListItem = array;
            }

            $.ajax({
                type: "POST",
                data: JSON.stringify(JSON_ARRAY),
                cache: false,
                async: false,
                dataType: 'json',
                contentType: 'application/json',
                url: '@Url.Action("CustomFieldSave", "Maintain")',
                success: function (msg) {
                    document.location = '@Url.Action("Maintain", "Maintain")';
                }
            });
            return false;
        });

    });
</script>



<script>


    //2016/06/15新增 如果資料剛LOADING 而TextType select 的是 日期欄位、時間欄位、
    $(document).ready(function () {
        var TextTypeValue = $('#TextType').val(); //抓取欄位樣式SelectValue

        $('#TextType_Hidden').val(TextTypeValue);
        


        getSelectedValueDate();    //選擇自訂時顯示額外選項
        ChangeITEM_CLASS_Time();
    });

    $(".menu-maintain").addClass("active");

    jQuery(function ($) {
        $('.switch-type input[type="checkbox"]').onoff();
    });


    $(".input-group").each(function () {
        var label_height = $(this).height();
        $(this).children(".form-control").css('height', label_height);
    });

    //選項隱藏
    $('.text-content').hide();
    $('.submit-btn').hide();


    //選擇自訂時顯示額外選項
    $('#TextType').change(function () { ChangeITEM_CLASS(); });
    function ChangeITEM_CLASS() {




        $('#main').find("input").removeAttr('required'); //remove所有的 required

        var selectedValue = $('#TextType option:selected').val();
        $('.submit-btn').show();

        $('.text-content').hide();
        $('#' + selectedValue).show();

        var inputFieldName = $('#' + selectedValue).find("input[name=FieldName]")
        var inputFieldLength = $('#' + selectedValue).find("input[name=FieldLength]")
        var inputFieldRowCount = $('#' + selectedValue).find("input[name=FieldRowCount]")
        var inputMinValue = $('#' + selectedValue).find("input[name=MinValue]")
        var inputMaxValue = $('#' + selectedValue).find("input[name=MaxValue]")
        var inputRepeatColumns = $('#' + selectedValue).find("input[name=RepeatColumns]")

        if (inputFieldName.attr('name') != 'undefined') {
            inputFieldName.prop('required', true);
        }
        if (inputFieldLength.attr('name') != 'undefined') {
            inputFieldLength.prop('required', true);
        }
        if (inputFieldRowCount.attr('name') != 'undefined') {
            inputFieldRowCount.prop('required', true);
        }
        if (inputMinValue.attr('name') != 'undefined') {
            inputMinValue.prop('required', true);
        }
        if (inputMaxValue.attr('name') != 'undefined') {
            inputMaxValue.prop('required', true);
        }
        if (inputRepeatColumns.attr('name') != 'undefined') {
            inputRepeatColumns.prop('required', true);
        }
    }

    //選擇自訂時顯示額外選項
    $('#DateType').change(function () { ChangeITEM_CLASS_Date(); });

    function ChangeITEM_CLASS_Date() {

        getSelectedValueDate();

    }
    function getSelectedValueDate() {
        var selectedValueDate = $('#DateType option:selected').val();

        if (selectedValueDate == "designation") {
            $('#DesignationDate').removeClass('dis-no');
            var FieldValueDate = document.getElementById('FieldValueDate')
            FieldValueDate.value = ""
        }
        else if (selectedValueDate == "preset") {

            var FieldValueDate = document.getElementById('FieldValueDate')
            FieldValueDate.value = "@@today"
            $('#DesignationDate').addClass('dis-no');


        }
        else {
            $('#DesignationDate').addClass('dis-no');
        }
    }






    //選擇自訂時顯示額外選項
    $('#TimeType').change(function () { ChangeITEM_CLASS_Time(); });
    function ChangeITEM_CLASS_Time() {
        var selectedValueTime = $('#TimeType option:selected').val();

        if (selectedValueTime == "designation") {
            $('#DesignationTime').removeClass('dis-no');
            var FieldValueDate = document.getElementById('FieldValueTime')
            FieldValueDate.value = "";
        }
        else if (selectedValueTime == "preset") {

            var FieldValueDate = document.getElementById('FieldValueTime')
            FieldValueDate.value = "@@today"
            $('#DesignationTime').addClass('dis-no');



        }
        else {
            $('#DesignationTime').addClass('dis-no');
        }


    }
</script>



