<style>
    .onoffswitch {
        display: inline-block;
        vertical-align: middle;
    }

    .onoffswitch-inner::before {
        content: "   @Html.BMResource("BM_Yes")";
    }

    .onoffswitch-inner::after {
        content: "   @Html.BMResource("BM_No")";
    }

    .onoffswitch-inner::before, .onoffswitch-inner::after {
        font-family: '微軟正黑體';
    }

    .onoffswitch-label {
        margin-bottom: 0;
    }


    .onoffswitch-switch, .onoffswitch-label {
        border: 1px solid #AAA;
    }

    .checkbox {
        display: inline-block;
        margin: 0;
    }

        .checkbox input {
            vertical-align: top;
        }

    .edit-wrap {
        padding-right: 30px;
    }

    .edit {
        background-color: #d9534f;
        color: #FFF !important;
        position: absolute;
        right: 0;
        top: 0;
        width: 30px;
        height: 100%;
        text-align: center;
        transition: all .3s;
    }


        .edit:hover {
            background-color: #c9302c;
        }

        .edit::after {
            content: "";
            display: inline-block;
            height: 100%;
            vertical-align: middle;
            width: 0;
        }

        .edit > i {
            display: inline-block;
            vertical-align: middle;
        }

    .tip {
        text-align: right;
        font-size: 0.975em;
        color: #757574;
        margin-top: 5px;
    }

    .input-group-addon, .input-group-btn {
        white-space: initial;
    }
</style>



<script>

    var Modal_Number = 0;

    function addListItem() {

        var txtModalInput = $('#txtModalInput').val();

        if (txtModalInput.value != "") {
            var content = "<label class='checkbox-inline' id='ModalListItemCheck" + Modal_Number + "'>";
            content += "<input type='checkbox' name='ModalListItemCheck'  value='" + txtModalInput.value + "'> " + txtModalInput.value;
            content += "</label>"

            $('#ModalListItem').append(content)
            Modal_Number++;
            $('#txtModalInput').val('');
        }
    }
    //點選取消後清空設定值
    function deleteListItem() {

        var l_ModalListItem = $('#CheckBoxModal').find("input[name^='ModalListItemCheck']:checked");
        var array = [];
        if (l_ModalListItem.length > 0) {
            for (var i = 0 ; i < l_ModalListItem.length; i++) {
                array.push(l_ModalListItem[i].parentNode.id);
            }
        }
        for (var i = 0; i < array.length; i++) {
            $("#" + array[i]).remove();
        }
    }

    //點選取消後清空設定值
    function CancelModalListItem() {

        $("#ModalListItem").empty();

    }


    function setEmpty() {
        var TextType = $("#TextType_Hidden").val();
        if (TextType == "RadioButtonList") {
            checkboxes = $('#RadioButtonListItem').find('input[type="radio"]:checked');
            for (var i = 0 ; i < checkboxes.length; i++) {          
                checkboxes[i].checked = false;
            }
        }
    }

    function AddModalListItem() {


       
        var TextType = $("#TextType_Hidden").val();

        var l_ModalListItem = $('#CheckBoxModal').find("input[name^='ModalListItemCheck']");
        var array = [];
        if (l_ModalListItem.length > 0) {
            for (var i = 0 ; i < l_ModalListItem.length; i++) {
                array.push(l_ModalListItem[i]);
            }
        }

        var content = "";
        if (TextType == "CheckBoxList") {
            $("#CheckBoxListItem").empty();
            for (var i = 0; i < array.length; i++) {

                content += " <label class='checkbox-inline'>";
                content += "               <input type='checkbox' id='inlineCheckbox" + i + "' name='ListItem' value='" + array[i].value + "'>" + array[i].value;
                content += "              </label>";
            }
    
            $("#CheckBoxListItem").append(content);
        }
        if (TextType == "RadioButtonList") {

            $("#RadioButtonListItem").empty();
            for (var i = 0; i < array.length; i++) {

                content += " <label class='radio-inline'>";
                content += "               <input type='radio' id='inlineRadio" + i + "' name='ListItem' value='" + array[i].value + "'>" + array[i].value;
                content += "              </label>";
            }
            $("#RadioButtonListItem").append(content);
        }
        if (TextType == "DropDownList") {

            $("#DropDownListItem").empty();
            for (var i = 0; i < array.length; i++) {

                content += " <input type='checkbox'  readonly='readonly' name='ListItem' id='dropOption1' value='" + array[i].value + "'>"
                content += array[i].value

            }
            $("#DropDownListItem").append(content);
        }





    }

    function TextTypeChange() {
        var txtType = $('#TextType').val(); //抓取欄位樣式SelectValue
        $("#TextType_Hidden").val(txtType);

    }

</script>

